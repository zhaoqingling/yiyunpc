/**翻译弹出**/
.dropdown{position: relative;width:260px;border: 1px solid #dbdbdb;cursor: pointer;background: #fff;border-radius: 3px;-webkit-user-select: none;-moz-user-select: none;user-select: none;}
.dropdown.open{z-index: 2;}
.dropdown .carat{position: absolute;right: 12px;top:55%;margin-top: -4px;border:4px solid transparent;border-top:4px solid #6a6c6f;}
.dropdown.open .carat{margin-top: -10px;border-top: 4px solid transparent;border-bottom: 4px solid #6a6c6f;}
.dropdown.disabled .carat{border-top-color: #999;}
.dropdown .old{position: absolute;left: 0;top: 0;height: 0;width: 0;overflow: hidden;}
.dropdown select{position: absolute;left: 0px;top: 0px;}
.dropdown.touch .old{width: 100%;height: 100%;}
.dropdown.touch select{width: 100%;height: 100%;opacity: 0;padding-left:10px}
.dropdown .selected,.dropdown li{display: block;font-size:14px;line-height: 1;color:#666;padding:11px 10px;overflow: hidden;white-space: nowrap;}
.dropdown .selected{height:44px;padding:15px 10px;}
.dropdown.disabled .selected{color: #999;}
.dropdown .selected::after{content: '';position: absolute;right: 0;top: 0;bottom: 0;width: 60px;border-radius: 0 2px 2px 0;}
.dropdown div{position: absolute;left: -1px;right: -1px;top:40px;background: #fff;border: 1px solid #ccc;*border:none;border:none\0;border-top: 1px solid #dbdbdb;*border-top:none;border-top:none\0;border-radius: 0 0 3px 3px;overflow: hidden;opacity:0;width:600px;max-height:200px;}
.dropdown.open div{opacity:1;z-index: 2;}
.dropdown.scrollable.bottom div::after{opacity: 0;}
.dropdown ul{position: absolute;left: 0;top: 0;width: 100%;list-style: none;}
.dropdown.scrollable.open ul{overflow-y: auto;}
.dropdown li{list-style: none;float:left;width:155px;text-align:center;margin-top:10px;margin-left:27px;font-size:14px;}
.dropdown li.focus:hover{background:#2965e6;position: relative;z-index: 3;color: #fff;}
.dropdown li.active{color:#2965e6;font-weight:bold}


