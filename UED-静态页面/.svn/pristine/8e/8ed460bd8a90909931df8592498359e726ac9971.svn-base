.p-title{margin: 0 auto;width: 153px;padding-top: 90px;padding-bottom: 15px;font-size: 28px;font-family: "PingFang SC";color: rgb(255, 253, 255);line-height: 0.857;text-align: center;border-bottom: solid 3px rgb(255, 255, 255);}
.activity-txt{width: 100%;font-size: 24px;font-family: "PingFang SC";color: rgb(255, 253, 255);line-height: 2;text-align: left;padding-bottom: 90px;}
.activity-txt ul{padding-left: 250px;padding-right: 250px;list-style: disc;color: white}
.activity-txt li span{font-size: 24px;font-family: "PingFang SC";color: rgb(255, 253, 255);line-height: 2;}
/*活动专题--英文*/
.p-title-En{margin: 0 auto;width: 153px;padding-top: 90px;padding-bottom: 15px;font-size: 32px;font-family: "Arial";color: rgb(255, 253, 255);font-weight: bold;line-height: 0.75;text-align: center;border-bottom: solid 3px rgb(255, 255, 255);}
.activity-txt-En{width: 100%;font-size: 24px;font-family: "Arial";color: rgb(255, 253, 255);line-height: 1.5;text-align: left;}
.activity-txt-En ul{padding-left: 230px;padding-right: 230px;list-style: disc;color: white;padding-bottom: 90px;}
.activity-txt-En li span{margin: 0 auto;font-size: 24px;font-family: "PingFang SC";color: rgb(255, 253, 255);line-height: 2;}

/*LSP人员管理*/
.lsp-member-manage .right-list{margin-top:0;}
.lsp-member-manage .member-title{*zoom:1;border-bottom:1px solid #e3e6ed;position:relative;}
.lsp-member-manage .member-title:after{content:" ";height:0;clear:both;display:block;}
.lsp-member-manage .member-title p{padding:0 0 18px 20px;line-height:22px;font-size:16px;color:#2c3e50;}
.lsp-member-manage .member-title button{position:absolute;top:0;right:0;}
.lsp-member-manage .member-box{padding:20px 0 14px 70px;position:relative;border-bottom:1px solid #e3e6ed;}
.lsp-member-manage .member-box p{line-height:30px;font-size:14px;color:#6a6c6f;}
.lsp-member-manage .member-box img{width:50px;height:50px;border:1px solid #0a5dd2;border-radius:50%;position:absolute;top:22px;left:0;}
.lsp-member-manage .member-box p.txt{font-size:16px;color:#2361ea;}
.lsp-member-manage .member-box p.txt span{margin-left:8px;padding:0 4px;color:#fe6b6b;border:1px solid #fe6b6b;font-size:12px;}
.lsp-member-manage .member-box p strong{font-weight:normal;}
.lsp-member-manage .member-box p .mr40{margin-right:40px;}
.lsp-member-manage .member-box p .mr20{margin-right:20px;}
.lsp-member-manage .member-box p .mr10{margin-right:10px;}
.lsp-member-manage .member-box p .red{color:#fe6b6b;}
.lsp-member-manage .member-box p .yellow{color:#ffb80d;}
.lsp-member-manage .member-box p .green{color:#39d5b0;}
.lsp-member-manage .member-box p.button-group{position:absolute;top:20px;right:0;}

/*消息*/
.topChoose{height: 50px;margin-top: 20px; background: #fff;font-size: 14px;color: #6a6c6f;}
.topChoose p{margin-left: 5px;float: left;cursor:pointer;}
.topChoose .deleteChoose{background-color: #fff; color: #517bed; margin-left: 25px; float: left;}
.topChoose .wechat{float: left;margin-left: 550px;color: #517bed;}
.topChoose .wechat a{color: #517bed;font-size: 14px}
.msg-input{padding-left: 20px;width:50px;position: relative;}
.msg-date{width: 190px;position: relative;}
.msg-date span {position: absolute;top: 15px;right: 130px}
.msg-date span img{margin:0 10px 2px 0;}
.msg-date p{margin-right: 110px;text-align: center;}
.msg-news{position:relative; width: 450px;}
.msg-news p{position: absolute;top: 15px;}
.msg-news p:hover{cursor: pointer;}
.msg-news div{margin-top:47px;line-height: 32px;text-align: justify;}
.msg-detail{display: none;}
.msg-select-check{height:20px;width:30px;background:url(../../images/checkbox.jpg) no-repeat 2px center;float:left;cursor:pointer;}
.msg-position{position: absolute;top:15px;left: 25px;}
.msg-select-check.active{background:url(../../images/checkbox-chek.jpg) no-repeat 2px center;}

/*LSP消息*/
.right-list-lsp1{width:100%;float:left;background:#fff;height:200px;}
.right-list-lsp1 .form-lable-lsp{width:520px;float:left;padding:20px 0 0 0;position:relative;}
.right-list-lsp1 .form-lable-lsp ul{width:520px;float:left;}
.right-list-lsp1 .form-lable-lsp ul li{width:520px;float:left;line-height:44px;margin-bottom:20px;}
.right-list-lsp1 .form-lable-lsp ul li p{float:left;margin-right:20px;position:relative;}
.right-list-lsp1 .form-lable-lsp ul li .word{width:100px;text-align: right;margin-right:20px;}
.right-list-lsp1 .lsg-logo{width: 140px;float: right;margin-top: 20px;}
.right-list-lsp1 .lsg-logo span{width:140px;display: block; }
.right-list-lsp1 .lsg-logo .btn-lsp{margin-top:10px;text-align:center;}
.right-list-lsp1 .lsg-logo .btn-lsp .btn{display:inline-block;line-height:23px;}
.right-list-lsp1 .lsg-logo .btn-lsp .webuploader-pick{padding:0;background:none;color:#2965e6;}
.right-list-lsp1 .lsg-logo .btn-lsp .webuploader-pick-hover{color:#fff;}
.right-list-lsp1:after{clear:both;display:block;content:"";height:0;}
.text-120{width:85%;height:150px;padding:0 10px 0 10px;line-height:28px;}

/*个人信息-认证后*/
.info-confirmed .word{border:0;float:left;}
.info-confirmed .value{width:750px;float:left;border-top:1px solid #e6e6e6;}
.info-confirmed .value .inputContainer p{float:none;line-height:60px;border-bottom:1px solid #e6e6e6;*zoom:1;}
.info-confirmed  p:after{content:" ";height:0;clear:both;display:block;}
.info-confirmed  p span{float:left;}
.info-confirmed  p span strong{font-weight:normal;}
.info-confirmed  p .txt1{padding-left:20px;width:200px;}
.info-confirmed  p .txt2{width:70px;}
.info-confirmed  p .txt3{padding-left:20px;width:260px;}
.info-confirmed  p input{margin-top:20px;float:right;}
.info-confirmed  p .input-sm{margin-top:0px;float:right;}
.info-confirmed  p input.btn-red{margin-left:10px;}
.info-confirmed  p input.btn-green{margin-left:10px;}
.info-confirmed .add-language{margin:14px 0 48px;padding-left:32px;line-height:24px;background:url(../../images/icon-add.png) no-repeat 5px 2px;font-size:13px;color:#39d5b0;display:inline-block;cursor:pointer;font-weight:bold;}
.info-confirmed .edit{padding:8px 0;border-bottom:1px solid #e6e6e6;*zoom:1;}
.info-confirmed .edit:after{content:" ";height:0;clear:both;display:block;}
.info-confirmed .edit ul li{float:none;margin-bottom:0;}
.info-confirmed .edit ul li .select-wrap{margin-right:30px;float:left;}
.info-confirmed .edit ul li .select-wrap1 .drop-down{width:175px;}
.info-confirmed .edit ul li .select-wrap1 .select-con{width:175px;}
.info-confirmed .edit ul li .select-wrap1 .select-con ul{width:175px;}
.info-confirmed .edit ul li .select-wrap2 .drop-down{width:92px;}
.info-confirmed .edit ul li .select-wrap2 .select-con{width:92px;}
.info-confirmed .edit ul li .select-wrap2 .select-con ul{width:92px;}
.info-confirmed .edit ul li .input1{width:92px;height:46px;float:left;}
.info-confirmed .edit ul li .unit{margin-left:12px;height:46px;line-height:46px;float:left;}
.info-confirmed .edit ul li .edit-chose{float:right;border-bottom:none;}
.info-confirmed .edit ul li .edit-chose span{width:35px;height:46px;display:inline-block;cursor:pointer;}
.info-confirmed .edit ul li .edit-chose span.txt4{background:url(../../images/icon-edit-finished.png) no-repeat center center;}
.info-confirmed .edit ul li .edit-chose span.txt5{background:url(../../images/icon-edit-close.png) no-repeat center center;}
/*微信二维码*/
.pass-list-erwm li{text-align: center;margin-bottom:25px}
.pass-list-erwm li img{margin-top: 25px;}
