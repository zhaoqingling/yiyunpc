/*广告位*/
.banner-link a{display: block!important;width: 100%;height: 100%;}
.advertisement-div{position:fixed;left: 0;bottom: 0;z-index: 1010;width:100%;height:130px;}
.advertisement-div .ads{height: 160px;}
.advertisement-div .ads a{display: block;height: 100%;}
.advertisement-div .ads-box{width: 1200px;margin: 0 auto; position: relative;top: 100px;left: 0;height: 30px;z-index: 1011;display: block!important;}
.advertisement-div .ads li{height: 160px;position: absolute;top: 0;left: 0;width: 100%;background-size: 100% auto;}
.advertisement-div .icon-close{width:16px;height:16px;background:url(../../images/close.png) no-repeat;position:absolute;top:-80px;right:26px;cursor:pointer;z-index: 1111;}
#adsNum{z-index: 1200;position: absolute;bottom: 10px;left: 50%;text-indent: 10000px;list-style: none;}
#adsNum li{float: left;width: 10px;height: 10px;border:1px solid #fff;border-radius: 50%;margin-right: 10px;text-indent: 10000px;}
#adsNum li.active{background: #fff;}
