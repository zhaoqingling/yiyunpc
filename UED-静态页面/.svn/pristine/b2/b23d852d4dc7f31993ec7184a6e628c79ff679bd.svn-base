<script type="text/javascript">
$(function(){

  /*点击checkbox*/
  $(document).on('click','.collect-con .select-check',function(){
      if(!$(this).hasClass('active')){
        $(this).addClass('active');
        $(this).parents('.collect-con').addClass('active');
      }else{
        $(this).removeClass('active');
         $(this).parents('.collect-con').removeClass('active');
      }
       return false;
  });

  /*点击全选*/
  $(document).on('click','[all-select]',function(){
    $('.translate-box .collect-con').each(function(){

        $(this).addClass('active');
        $(this).find('.select-check').addClass('active');
    });
     return false;
  });
  /*点击取消*/
  $(document).on('click','[all-cancel]',function(){
    $('.translate-box .collect-con').each(function(){

        $(this).removeClass('active');
        $(this).find('.select-check').removeClass('active');
    });
     return false;
  });
});
</script>
